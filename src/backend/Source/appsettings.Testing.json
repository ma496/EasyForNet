{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "RoutePrefix": "api",
  "Auth": {
    "Jwt": {
      "Key": "a long secret string used to sign jwts. usually set via matching environment variable.",
      "Issuer": "the issuer of the jwt",
      "Audience": "the audience of the jwt"
    },
    "AccessTokenValidity": "60",
    "RefreshTokenValidity": "168"
  },
  "Payload": {
    "MaximumSize": "26214400"
  },
  "File": {
    "DeleteUnactiveFilesTime": "24"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=EasyForNetTest;Username=postgres;Password=123456"
  }
}